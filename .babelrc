{
  "env": {
    "cjs": {
      "presets": [["@babel/preset-env"]],
      "plugins": [
        ["@babel/plugin-transform-runtime", { "corejs": 2 }],
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "es": {
      "presets": [["@babel/preset-env", { "modules": false }]],
      "plugins": [
        ["@babel/plugin-transform-runtime", { "corejs": 2 }],
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
