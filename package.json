{
  "name": "moducks",
  "version": "1.0.1",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "check-types": "tsc",
    "lint:src:fix": "eslint --fix --ext .ts ./src",
    "lint:src": "eslint src --ext .ts ./src",
    "lint:examples:fix": "eslint --fix --ext .ts ./examples",
    "lint:examples": "eslint src --ext .ts ./examples",
    "test": "npm run lint-src && cross-env BABEL_ENV=cjs babel-node test/index.js | tap-spec",
    "test-without-lint": "cross-env BABEL_ENV=cjs babel-node test/index.js | tap-spec",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "run-p build:*",
    "prepublishOnly": "git diff --no-ext-diff --quiet --exit-code src .gitignore package.json README.md LICENSE || (echo \"Please commit changes!\" && false) && npm test && npm run build"
  },
  "author": "rzhx",
  "contributors": [
    "mpyw <ryosuke_i_628@yahoo.co.jp>"
  ],
  "license": "MIT",
  "description": "Ducks (Redux Reducer Bundles) + Redux-Saga = Moducks",
  "keywords": [
    "ducks",
    "redux-actions",
    "redux-saga",
    "redux",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moducks/moducks.git"
  },
  "bugs": {
    "url": "https://github.com/moducks/moducks/issues"
  },
  "homepage": "https://github.com/moducks/moducks#readme",
  "dependencies": {
    "redux-actions": "^2.6.5",
    "to-camel-case": "^1.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime-corejs2": "^7.6.0",
    "@types/redux-actions": "^2.6.1",
    "@types/to-camel-case": "^1.0.0",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "typescript": "^3.6.3"
  }
}
